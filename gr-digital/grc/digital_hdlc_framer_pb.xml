<?xml version="1.0"?>
<block>
  <name>HDLC Framer</name>
  <key>digital_hdlc_framer_pb</key>
  <import>from gnuradio import digital</import>
  <make>digital.hdlc_framer_pb($frame_tag_name)</make>
  <param>
    <name>Frame tag name</name>
    <key>frame_tag_name</key>
    <type>string</type>
  </param>
  <sink>
    <name>in</name>
    <type>message</type>
  </sink>
  <source>
    <name>out</name>
    <type>byte</type>
  </source>
</block>
