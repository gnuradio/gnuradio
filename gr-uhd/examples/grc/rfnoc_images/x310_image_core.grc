    NUM_HB: '3'
    NUM_PORTS: '1'
    affinity: ''
    alias: duc0
    comment: ''
    desc: duc.yml
    maxoutbuf: '0'
    minoutbuf: '0'
    type: block
  states:
    bus_sink: false
    bus_source: false
    bus_structure: null
    coordinate: [664, 72.0]
    rotation: 0
    state: enabled
