 -->
<block>
	<name>VCO</name>
	<key>gr_vco_f</key>
	<import>from gnuradio import gr</import>
	<make>gr.vco_f($samp_rate, $sensitivity, $amplitude)</make>
	<param>
		<name>Sample Rate</name>
		<key>samp_rate</key>
		<type>real</type>
	</param>
	<param>
		<name>Sensitivity</name>
		<key>sensitivity</key>
		<type>real</type>
	</param>
	<param>
		<name>Amplitude</name>
		<key>amplitude</key>
		<type>real</type>
	</param>
	<sink>
		<name>in</name>
		<type>float</type>
	</sink>
	<source>
		<name>out</name>
		<type>float</type>
	</source>
</block>
