<block>
	<name>Phase Mod</name>
	<key>gr_phase_modulator_fc</key>
	<import>from gnuradio import gr</import>
	<make>gr.phase_modulator_fc($sensitivity)</make>
	<param>
		<name>Sensitivity</name>
		<key>sensitivity</key>
		<type>real</type>
	</param>
	<sink>
		<name>in</name>
		<type>float</type>
	</sink>
	<source>
		<name>out</name>
		<type>complex</type>
	</source>
</block>
