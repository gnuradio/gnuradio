<?xml version="1.0"?>
<!--
###################################################
##PLL Carrier Tracking
###################################################
 -->
<block>
	<name>PLL Carrier Tracking</name>
	<key>gr_pll_carriertracking_cc</key>
	<import>from gnuradio import gr</import>
	<make>gr.pll_carriertracking_cc($w, $max_freq, $min_freq)</make>
	<callback>set_loop_bandwidth($w)</callback>
	<param>
		<name>Loop Bandwidth</name>
		<key>w</key>
		<type>real</type>
	</param>
	<param>
		<name>Max Freq</name>
		<key>max_freq</key>
		<type>real</type>
	</param>
	<param>
		<name>Min Freq</name>
		<key>min_freq</key>
		<type>real</type>
	</param>
	<sink>
		<name>in</name>
		<type>complex</type>
	</sink>
	<source>
		<name>out</name>
		<type>complex</type>
	</source>
</block>
