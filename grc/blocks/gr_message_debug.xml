###################################################
 -->
<block>
	<name>Message Debug</name>
	<key>gr_message_debug</key>
	<import>from gnuradio import gr</import>
	<make>gr.message_debug()</make>
	<sink>
		<name>print</name>
		<type>message</type>
		<optional>1</optional>
	</sink>
	<sink>
		<name>store</name>
		<type>message</type>
		<optional>1</optional>
	</sink>
	<sink>
		<name>print_pdu</name>
		<type>message</type>
        <optional>1</optional>
	</sink>
</block>
