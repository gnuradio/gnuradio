<block>
    <name>testname</name>
    <key>block_key</key>
    <make>blocks.complex_to_mag_squared($(vlen))</make>
    <param>
        <name>Vec Length</name>
        <key>vlen</key>
        <value>1</value>
        <type>int</type>
        <tab>test</tab>
    </param>
    <param>
        <name>Vec Length</name>
        <key>out_type</key>
        <value>complex</value>
        <type>string</type>
    </param>
    <param>
        <name>Alpha</name>
        <key>a</key>
        <value>0</value>
        <type>($out_type)</type>
    </param>
    <check>$vlen &gt; 0</check>
    <sink>
        <name>in</name>
        <type>complex</type>
        <vlen>2 * $vlen</vlen>
    </sink>
    <sink>
        <name>in2</name>
        <type>message</type>
    </sink>
    <source>
        <name>out</name>
        <type>$out_type</type>
        <vlen>$vlen</vlen>
    </source>
</block>
