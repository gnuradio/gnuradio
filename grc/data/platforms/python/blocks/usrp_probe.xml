<?xml version="1.0"?>
<!--
###################################################
##USRP Probe:
##	This block should not appear in the tree.
###################################################
 -->
<block>
	<name>USRP Probe</name>
	<key>usrp_probe</key>
	<make></make>
	<param>
		<name>USRP Number</name>
		<key>number</key>
		<value>0</value>
		<type>int</type>
	</param>
	<param>
		<name>Daughter Board</name>
		<key>dboard</key>
		<type>enum</type>
		<option>
			<name>RX A</name>
			<key>rx_a</key>
		</option>
		<option>
			<name>RX B</name>
			<key>rx_b</key>
		</option>
		<option>
			<name>TX A</name>
			<key>tx_a</key>
		</option>
		<option>
			<name>TX B</name>
			<key>tx_b</key>
		</option>
	</param>
	<doc>Press "Probe" to retrieve USRP information...</doc>
</block>
