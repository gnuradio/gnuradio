<block>
	<name>Unpack K Bits</name>
	<key>gr_unpack_k_bits_bb</key>
	<import>from gnuradio import gr</import>
	<make>gr.unpack_k_bits_bb($k)</make>
	<param>
		<name>K</name>
		<key>k</key>
		<type>int</type>
	</param>
	<sink>
		<name>in</name>
		<type>byte</type>
	</sink>
	<source>
		<name>out</name>
		<type>byte</type>
	</source>
</block>
