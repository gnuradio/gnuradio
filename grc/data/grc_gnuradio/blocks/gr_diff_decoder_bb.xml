<?xml version="1.0"?>
<!--
###################################################
##Differential Decoder
###################################################
 -->
<block>
	<name>Differential Decoder</name>
	<key>gr_diff_decoder_bb</key>
	<import>from gnuradio import gr</import>
	<make>gr.diff_decoder_bb($modulus)</make>
	<param>
		<name>Modulus</name>
		<key>modulus</key>
		<type>int</type>
	</param>
	<sink>
		<name>in</name>
		<type>byte</type>
	</sink>
	<source>
		<name>out</name>
		<type>byte</type>
	</source>
</block>
