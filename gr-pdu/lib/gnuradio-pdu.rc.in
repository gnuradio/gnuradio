        VALUE "FileDescription", "gnuradio-pdu"
        VALUE "FileVersion", "@VERSION@"
        VALUE "InternalName", "gnuradio-pdu.dll"
        VALUE "LegalCopyright", "Licensed under GPLv3 or any later version"
