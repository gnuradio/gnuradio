<!--
###################################################
##CTCSS Squelch
###################################################
 -->
<block>
	<name>CTCSS Squelch</name>
	<key>analog_ctcss_squelch_ff</key>
	<import>from gnuradio import analog</import>
	<make>analog.ctcss_squelch_ff($rate, $freq, $level, $len, $ramp, $gate)</make>
	<callback>set_level($level)</callback>
	<callback>set_frequency($freq)</callback>
	<param>
		<name>Sampling Rate (Hz)</name>
		<key>rate</key>
		<value>samp_rate</value>
		<type>real</type>
	</param>
	<param>
		<name>Tone Frequency</name>
		<key>freq</key>
		<value>100.0</value>
		<type>real</type>
	</param>
	<param>
		<name>Level</name>
		<key>level</key>
		<value>0.01</value>
		<type>real</type>
	</param>
	<param>
		<name>Length</name>
		<key>len</key>
		<value>0</value>
		<type>int</type>
	</param>
	<param>
		<name>Ramp</name>
		<key>ramp</key>
		<value>0</value>
		<type>int</type>
	</param>
	<param>
		<name>Gate</name>
		<key>gate</key>
		<value>False</value>
		<type>bool</type>
	</param>
	<sink>
		<name>in</name>
		<type>float</type>
	</sink>
	<source>
		<name>out</name>
		<type>float</type>
	</source>
</block>
