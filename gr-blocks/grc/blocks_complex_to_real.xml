<?xml version="1.0"?>
<!--
###################################################
##Complex to Real:
##	one or two output streams
###################################################
 -->
<block>
	<name>Complex To Real</name>
	<key>blocks_complex_to_real</key>
	<import>from gnuradio import blocks</import>
	<make>blocks.complex_to_real($vlen)</make>
	<param>
		<name>Vec Length</name>
		<key>vlen</key>
		<value>1</value>
		<type>int</type>
	</param>
	<check>$vlen &gt; 0</check>
	<sink>
		<name>in</name>
		<type>complex</type>
		<vlen>$vlen</vlen>
	</sink>
	<source>
		<name>re</name>
		<type>float</type>
		<vlen>$vlen</vlen>
	</source>
</block>
