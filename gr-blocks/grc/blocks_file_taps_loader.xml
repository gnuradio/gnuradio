<?xml version="1.0"?>
<!--
###################################################
##File Taps Loader
###################################################
 -->
<block>
  <name>File Taps Loader</name>
  <key>blocks_file_taps_loader</key>
  <category>[File Operators]</category>
  <import>from gnuradio import blocks</import>
  <make>blocks.file_taps_loader($file, $verbose)</make>
  <param>
    <name>File</name>
    <key>file</key>
    <value></value>
    <type>file_open</type>
  </param>
  <param>
    <name>Verbose Output</name>
    <key>verbose</key>
    <type>bool</type>
    <option>
            <name>YES</name>
            <key>True</key>
    </option>
    <option>
            <name>NO</name>
            <key>False</key>
    </option>
  </param>
</block>
