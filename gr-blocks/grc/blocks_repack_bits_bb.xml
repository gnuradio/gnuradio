<block>
  <name>Repack Bits</name>
  <key>blocks_repack_bits_bb</key>
  <import>from gnuradio import blocks</import>
  <make>blocks.repack_bits_bb($k, $l, $len_tag_key, $align_output)</make>
  <param>
    <name>Bits per input byte</name>
    <key>k</key>
    <value>1</value>
    <type>int</type>
  </param>
  <param>
    <name>Bits per output byte</name>
    <key>l</key>
    <value>8</value>
    <type>int</type>
  </param>
  <param>
    <name>Length Tag Key</name>
    <key>len_tag_key</key>
    <value>""</value>
    <type>string</type>
  </param>
  <param>
    <name>Packet Alignment</name>
    <key>align_output</key>
    <value>False</value>
    <type>enum</type>
    <option>
        <name>Output</name>
        <key>True</key>
    </option>
    <option>
        <name>Input</name>
        <key>False</key>
    </option>
  </param>
  <sink>
    <name>in</name>
    <type>byte</type>
  </sink>
  <source>
    <name>out</name>
    <type>byte</type>
  </source>
</block>
